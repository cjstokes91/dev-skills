<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/style">
</head>

<body>
    <div>
        <h1><%= devName %>'s Developer Skills</h1>
        <ul>
            <% skills.forEach(function(s, idx) { %>
            <li>
                <a href="/skills/<%=idx%>"><%= s.skill%></a>
                <%= s.skill%>
                -
                <%= s.learned ? 'learned' : 'work in progress' %>
            </li>

            <%  }); %>
        </ul>
        <a href="skills/new">Add Dev Skills</a>
    </div>

</body>

</html>